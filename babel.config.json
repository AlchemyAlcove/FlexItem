{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "last 2 years",
        "useBuiltIns": "entry",
        "bugfixes": true,
        "corejs": 3
      }
    ],
    [
      "@babel/preset-react",
      { "runtime": "automatic", "importSource": "@emotion/react" }
    ]
  ],
  "plugins": [
    "@emotion/babel-plugin",
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "esm": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": "last 2 years",
            "bugfixes": true,
            "useBuiltIns": "entry",
            "corejs": 3,
            "modules": false
          }
        ],
        [
          "minify",
          {
            "builtIns": false,
            "mangle": false
          }
        ]
      ]
    }
  },
  "comments": false
}
