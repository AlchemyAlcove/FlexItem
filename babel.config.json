{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "last 2 years",
        "useBuiltIns": "entry",
        "bugfixes": true,
        "corejs": 3
      }
    ],
    "@babel/preset-react",
    [
      "@emotion/babel-preset-css-prop",
      {
        "autoLabel": "always",
        "labelFormat": "[local]",
        "sourceMap": false
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3,
        "version": "7.12.10"
      }
    ]
  ],
  "env": {
    "esm": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": "last 2 years",
            "bugfixes": true,
            "useBuiltIns": "entry",
            "corejs": 3,
            "modules": false
          }
        ],
        [
          "minify",
          {
            "builtIns": false,
            "mangle": false
          }
        ]
      ],
      "plugins": [
        [
          "@babel/plugin-transform-runtime",
          {
            "useEsModules": true
          }
        ]
      ]
    }
  },
  "comments": false
}
