{
  "name": "flex-item",
  "version": "4.0.0-beta.0",
  "private": false,
  "description": "React components for styling grids with css flexboxes. Uses emotion under the hood.",
  "author": "AlchemyAlcove",
  "license": "MIT",
  "keywords": [
    "flexbox",
    "flexboxes",
    "flexboxs",
    "react",
    "emotion",
    "grid"
  ],
  "main": "index.js",
  "module": "dist/esm/index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AlchemyAlcove/FlexItem"
  },
  "scripts": {
    "start": "webpack serve --config webpack.config.dev.js",
    "lint": "eslint src",
    "lint:css": "stylelint \"./**/*.js\"",
    "build": "npm-run-all --parallel build:*",
    "prebuild:webpack": "rimraf dist/cjs dist/umd",
    "build:webpack": "npm-run-all --parallel webpack:*",
    "webpack:dev": "webpack --mode development",
    "webpack:prod": "cross-env NODE_ENV=production webpack --mode production",
    "prebuild:esm": "rimraf dist/esm",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir dist/esm",
    "performance": "cross-env NODE_ENV=production node_modules/.bin/webpack --config webpack.config.dev.js --progress"
  },
  "dependencies": {
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/eslint-parser": "7.12.1",
    "@babel/eslint-plugin": "7.12.1",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@emotion/babel-preset-css-prop": "11.0.0",
    "@emotion/eslint-plugin": "11.0.0",
    "babel-loader": "8.2.2",
    "babel-preset-minify": "0.5.1",
    "clean-webpack-plugin": "3.0.0",
    "core-js": "3.8.2",
    "cross-env": "7.0.3",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-config-standard": "16.0.2",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-standard": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "stylelint": "13.8.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-custom-processor-loader": "0.6.0",
    "stylelint-prettier": "1.1.2",
    "stylelint-processor-styled-components": "1.10.0",
    "webpack": "4.45.0",
    "webpack-bundle-analyzer": "4.3.0",
    "webpack-cli": "4.3.1",
    "webpack-dev-server": "3.11.1"
  }
